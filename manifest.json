{
    "name": "Session Saver",
    "description": "Save a list of all the tabs you currently have open in your window so you can open them later.",
    "version": "1.0.0",
    "manifest_version": 3,
    
    "action": {
        "default_title": "Session Saver",
        "default_popup": "popup.html",
        "default_icon": {
            "16":"SessionsSaverIcon.png",
            "32":"SessionsSaverIcon.png",
            "48":"SessionsSaverIcon.png",
            "128":"SessionsSaverIcon.png"
        }
    },
    "icons": {
        "16":"SessionsSaverIcon.png",
        "32":"SessionsSaverIcon.png",
        "48":"SessionsSaverIcon.png",
        "128":"SessionsSaverIcon.png"
    },
    "permissions": [
        "sessions",
        "tabs",
        "storage"
    ]
    
}